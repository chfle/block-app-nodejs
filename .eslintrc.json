{
  "extends": ["airbnb", "plugin:jsx-a11y/recommended"],
  "plugins": ["jsx-a11y"],
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "rules": {
    "no-console": "off",
    "strict": "off",
    "no-unused-vars": "off",
    "no-underscore-dangle": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/forbid-prop-types": "off",
    "max-len": "off",
    "no-undef": "off",
    "global-require": "off",
    "func-names": "off",
    "prefer-destructuring": "off",
    "import/no-extraneous-dependencies": ["error", {"packageDir": "./package.json/"}],
    "yield-star-spacing": "off"
  }
}
